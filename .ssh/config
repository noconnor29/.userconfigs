    IdentityFile ~/.ssh/%l
    IdentitiesOnly yes
    PreferredAuthentications publickey

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/%l
    IdentitiesOnly yes
    PreferredAuthentications publickey
    
