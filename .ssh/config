Host jump
    HostName jump.nodv.info
    ProxyJump None
    IdentityFile ~/.ssh/%l
    IdentitiesOnly yes
    PreferredAuthentications publickey

Host github.com
    HostName github.com
    User git
    ProxyJump jump
    IdentityFile ~/.ssh/%l
    IdentitiesOnly yes
    PreferredAuthentications publickey

Host *
    User adminnick
    IdentityFile ~/.ssh/%l
    LogLevel error
    ProxyJump jump
    IdentitiesOnly yes
    PreferredAuthentications publickey
    ServerAliveInterval 240
